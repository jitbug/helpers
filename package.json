{
  "name": "@jitbug/stencil-helpers",
  "version": "1.0.0",
  "description": "Stencil helpers",
  "author": "Jitbug Ltd",
  "repository": "jitbug/stencil-helpers",
  "license": "MIT",
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --serve --watch",
    "start:sw": "npm run start --service-worker",
    "lint": "tslint --project .",
    "test": "stencil test --next --spec",
    "deploy": "npm publish"
  },
  "dependencies": {
    "@capacitor/core": "1.5.0",
    "@ionic/core": "4.11.10",
    "@ionic/pwa-elements": "1.4.2",
    "moment": "2.24.0",
    "moment-timezone": "0.5.27",
    "nprogress": "0.2.0"
  },
  "devDependencies": {
    "@stencil/core": "1.9.0-13",
    "@stencil/stylus": "1.0.0",
    "@types/jest": "24.9.1",
    "@types/moment-timezone": "0.5.12",
    "@types/node": "13.7.1",
    "@types/nprogress": "0.2.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "rollup": "1.31.1",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-xo": "0.16.0",
    "workbox-build": "4.3.1"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": true,
    "trailingComma": "all",
    "bracketSpacing": true
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@jitbug"
  }
}
